{
  "extends": "takiyon-react",
  "env": {
    "browser": true,
    "mocha": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.test.config.js"
      }
    }
  },
  "rules": {
    "react/destructuring-assignment": "off",
    "react/jsx-sort-props": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "react/sort-prop-types": "off"
  }
}
